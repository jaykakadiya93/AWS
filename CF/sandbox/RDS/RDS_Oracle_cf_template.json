{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "RDS Oracle Instance Creation",
  "Parameters": {
    "Engine": {
      "Description": "RDS Engine Name",
      "Type": "String"
    },
    "DBInstanceClass": {
      "Description": "RDS DBInstanceClass",
      "Type": "String"
    },
    "DBName": {
      "Description": "RDS DBName Name",
      "Type": "String"
    },
    "MasterUsername": {
      "Description": "RDS MasterUsername",
      "Type": "String"
    },
    "MasterUserPassword": {
      "Description": "RDS MasterUserPassword",
      "Type": "String"
    },
    "AllocatedStorage": {
      "Description": "RDS AllocatedStorage",
      "Type": "String"
    },
    "DBInstanceIdentifier": {
      "Description": "RDS DBInstanceIdentifier Name",
      "Type": "String"
    },
    "AvailabilityZone": {
      "Description": "RDS AvailabilityZone Name",
      "Type": "String"
    }
  },
  "Resources": {
    "RDSInstance": {
      "Type": "AWS::RDS::DBInstance",
      "Properties": {
        "Engine": {
          "Ref": "Engine"
        },
        "LicenseModel": "license-included",
        "EngineVersion": "12.1.0.2.v15",
        "DBInstanceClass": {
          "Ref": "DBInstanceClass"
        },
        "AvailabilityZone": {
          "Ref": "AvailabilityZone"
        },
        "DBName": {
          "Ref": "DBName"
        },
        "MasterUsername": {
          "Ref": "MasterUsername"
        },
        "MasterUserPassword": {
          "Ref": "MasterUserPassword"
        },
        "AllocatedStorage": {
          "Ref": "AllocatedStorage"
        },
        "DBInstanceIdentifier": {
          "Ref": "DBInstanceIdentifier"
        }
      }
    }
  }
}